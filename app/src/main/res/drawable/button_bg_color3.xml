<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_pressed="true" >
        <shape android:shape="rectangle">
<!--            <solid android:color="@color/colorStatePressed" />-->
            <gradient android:startColor="#ACD6D6" android:endColor="#2657A1"  android:angle="-90.0"/>
            <corners android:radius="8dp" />
        </shape>
    </item>

    <item android:drawable="@drawable/ic_vector1">
        <solid android:color="@color/colorStateEnabled" />

    </item>

    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorStateEnabled" />
            <corners android:radius="8dp" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorStateNotEnabled" />
            <corners android:radius="8dp" />
        </shape>
    </item>

</selector>